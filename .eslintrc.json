{
  "extends": "google",
  "env": {
      "browser": true,
      "jasmine": true,
      "node": true,
      "mocha": true
  },
  "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": false
      }
  },
  "rules": {
      "semi": 2,
      "no-fallthrough": 2,
      "curly": [2, "multi-line"],
      "require-jsdoc": 0,
      "one-var": 0,
      "linebreak-style": 0,
      "no-var": 0,
      "arrow-parens": 0,
      "object-curly-spacing": [1, "always"],
      "no-invalid-this": 0,
      "no-unused-vars": 1,
      "prefer-rest-params": 0,
      "prefer-spread": 0,
      "no-undef": 2,
      "no-multi-spaces": 0,
      "max-len": [1, { "code": 80, "ignoreUrls": true }],
      "quote-props": [2, "as-needed"],
      "no-cond-assign": "error"
  },
  "globals": {
    "Map": false,
    "Set": false,
    "Symbol": false,
    "angular": false,
    "location": false,
    "console": false,
    "describe": false,
    "browser": false,
    "element": false,
    "by": false,
    "Promise": false
  }
}
